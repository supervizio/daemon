<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>io.superviz.daemon</string>
    <key>Program</key>
    <string>/usr/local/bin/supervizio</string>
    <key>ProgramArguments</key>
    <array>
        <string>/usr/local/bin/supervizio</string>
        <string>--config</string>
        <string>/etc/supervizio/config.yaml</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/var/log/supervizio.log</string>
    <key>StandardErrorPath</key>
    <string>/var/log/supervizio.err</string>
</dict>
</plist>
